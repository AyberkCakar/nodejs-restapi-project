{
  "name": "myfis-service",
  "version": "1.1.0",
  "main": "./build/app.js",
  "scripts": {
    "start": "npm run build && node ./build/app.js -p $PORT",
    "start:dev": "nodemon src/app.js --exec babel-node",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s --copy-files",
    "clean": "rm -rf ./build && mkdir build",
    "heroku-postbuild": "npm install --only=dev && npm run build",
    "release": "standard-version"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.2",
    "standard-version": "^9.0.0"
  }
}
